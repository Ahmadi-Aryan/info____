int v[128];
int v2[128];
int n;
int nr=0;
void citire()
{
    cin>>n;
}
void to_bin(){
    while(n!=0){
        v[nr]=n%2;
        nr++;
        n/=2;
    }
}
void change_vector(){
    for(int i = 0; i< nr; i++)
    {
        v2[nr-1-i]=v[i];
    }
}
void afisare()
{
    for (int i = 0; i<nr; i++)
    {
        cout<<v2[i]<<" ";
    }int nr=0;
    cout<<endl;
}
int main(){
    citire();
    to_bin();
    change_vector();
    afisare();
}
